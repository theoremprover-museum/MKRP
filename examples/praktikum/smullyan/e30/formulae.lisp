(* AXIOMS OF PROBLEM 30)
(*)
(* THERE IS JUST ONE PERSON,A.)
(* A SAYS: EITHER I AM A KNAVE OR ELSE TWO PLUS TWO EQUALS FIVE.)
(*)
(* EITHER2 (X) MEANS X SAYS : EITHER I AM A KNAVE OR FALSE.)

(ALL X KNIGHT (X) AND EITHER2 (X) IMPL (KNAVE (X) OR FALSE))
(ALL X KNAVE (X) AND EITHER2 (X) IMPL NOT (KNAVE (X) OR FALSE))
(ALL X
KNIGHT (X) EQV NOT KNAVE (X))
(EITHER2 (A))


