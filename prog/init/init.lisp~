(IN-PACKAGE "MARKGRAF-KARL" :use '("CL") :nicknames '("MKRP"))

(defun mkrp-init ()
  #+symbolics
  (OPT-WND_INIT)
  (OPT-SET.STANDARD)
  (MEM-INITIALIZE 10000)
  (edt-set.readtables))